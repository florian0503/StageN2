{
  "kind": "collectionType",
  "collectionName": "rooms",
  "info": {
    "singularName": "room",
    "pluralName": "rooms",
    "displayName": "Room"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "string"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "capacity": {
      "type": "integer"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "room"
    }
  }
}
